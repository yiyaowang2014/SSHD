<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.codehaus.mojo</groupId>
    <artifactId>mojo-parent</artifactId>
    <version>31</version>
  </parent>

  <groupId>org.codehaus.mojo.signature</groupId>
  <artifactId>signatures-parent</artifactId>
  <version>1.2</version>
  <packaging>pom</packaging>

  <name>API Signatures Parent project</name>

  <description>
    The API Signatures project contains a number of projects which generate
    signatures of various APIs, such as the Java Runtime.  These signatures
    are generated by and consumed by the Animal Sniffer project.
  </description>

  <url>http://mojo.codehaus.org/signatures</url>

  <distributionManagement>
    <site>
      <id>codehaus.org</id>
      <name>Mojo Website</name>
      <url>dav:https://dav.codehaus.org/mojo/signatures</url>
    </site>
  </distributionManagement>

  <inceptionYear>2009</inceptionYear>

  <licenses>
    <license>
      <name>MIT license</name>
      <url>http://www.opensource.org/licenses/mit-license.php</url>
      <distribution>repo</distribution>
    </license>
  </licenses>

  <issueManagement>
    <system>jira</system>
    <url>http://jira.codehaus.org/browse/MANIMALSNIFFER</url>
  </issueManagement>

  <scm>
    <connection>scm:svn:https://svn.codehaus.org/mojo/tags/signatures-parent-1.2</connection>
    <developerConnection>scm:svn:https://svn.codehaus.org/mojo/tags/signatures-parent-1.2</developerConnection>
    <url>http://fisheye.codehaus.org/browse/mojo/tags/signatures-parent-1.2</url>
  </scm>

  <developers>
    <developer>
      <name>Kohsuke Kawaguchi</name>
      <email>kohsuke (dot) kawaguchi (at) sun (dot) com</email>
      <roles>
        <role>Lead Developer</role>
      </roles>
      <timezone>-8</timezone>
    </developer>
    <developer>
      <name>Stephen Connolly</name>
      <email>stephen (dot) alan (dot) connolly (at) gmail (dot) com</email>
      <roles>
        <role>Developer</role>
      </roles>
      <timezone>0</timezone>
    </developer>
    <developer>
      <name>Arnaud Heritier</name>
      <email>aheritier (at) apache (dot) org</email>
      <roles>
        <role>Developer</role>
      </roles>
      <timezone>+1</timezone>
    </developer>
    <developer>
      <name>David J. M. Karlsen</name>
      <email>david@codehaus.org</email>
      <roles>
        <role>Developer</role>
      </roles>
      <timezone>1</timezone>
    </developer>
    <developer>
      <name>Tony Chemit</name>
      <email>tchemit (at) codehaus (dot) org</email>
      <roles>
        <role>Developer</role>
      </roles>
      <timezone>Europe/Paris</timezone>
    </developer>

  </developers>

  <properties>
    <!-- referenced from the signatures index.apt.vm -->
    <animal-sniffer-version>1.9</animal-sniffer-version>
  </properties>

  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.codehaus.mojo</groupId>
          <artifactId>animal-sniffer-maven-plugin</artifactId>
          <version>${animal-sniffer-version}</version>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>

</project>
